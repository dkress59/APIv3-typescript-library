{
  "compilerOptions": {
    "outDir": "dist/",
    "module": "commonjs",
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true,
    "target": "ES2015",
    "strict": true,
    "moduleResolution": "node",
    "removeComments": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "declaration": true,
    "types": []
  },
  "include": ["api/", "model/"]
}
